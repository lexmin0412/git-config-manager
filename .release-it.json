{
	"git": {
		"commitMessage": "chore(release): v${version}"
	},
	"github": {
		"release": true,
		"releaseName": "v${version}",
		"tokenRef": "COMMIT_TOKEN"
	},
	"npm": {
		"publish": true,
		"publishPath": ".",
		"publishArgs": [],
		"tag": null,
		"otp": null,
		"ignoreVersion": false,
		"allowSameVersion": false,
		"versionArgs": [],
		"skipChecks": false,
		"timeout": 10
	},
	"plugins": {
		"@release-it/conventional-changelog": {
			"infile": "CHANGELOG.md",
			"preset": {
				"name": "conventionalcommits",
				"types": [
					{
						"type": "feat",
						"section": "Features"
					},
					{
						"type": "fix",
						"section": "Bug Fixes"
					},
					{}
				]
			}
		}
	}
}
